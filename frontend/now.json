{
  "version": 2,
  "name": "ashhitch",
  "builds": [{ "src": "next.config.js", "use": "@now/next", "config": { "newPipeline": true } }],
  "routes": [
    { "src": "^/post/(?<slug>[^/]*)", "dest": "/post?apiRoute=post&slug=$slug" },
    { "src": "^/work/(?<slug>[^/]*)", "dest": "/post?apiRoute=work&slug=$slug" },
    { "src": "^/page/(?<slug>[^/]*)", "dest": "/post?apiRoute=page&slug=$slug" }
  ],
  "alias": ["dev.ashleyhitchcock.com"],
  "github": {
    "enabled": false
  },
  "env": {
    "NODE_ENV": "production"
  }
}
